{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://3.216.98.136:8000/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/trigger-scrape",
      "schedule": "0 1 * * *"
    }
  ],
  "github": {
    "enabled": true,
    "silent": false
  },
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FORCE_COLOR": "1"
  }
} 
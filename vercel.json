{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/parse.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_KEY": "@supabase_service_key",
    "THIRD_PARTY_API_KEY": "@third_party_api_key"
  }
} 
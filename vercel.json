{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/parse.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "github": {
    "enabled": true,
    "silent": false
  },
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FORCE_COLOR": "1"
  }
} 
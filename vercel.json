{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "silent": false
  },
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FORCE_COLOR": "1"
  }
} 
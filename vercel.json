{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "${VERCEL_API_URL}/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "silent": false
  },
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FORCE_COLOR": "1"
  }
} 
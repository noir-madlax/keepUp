{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
        "app/main.py": {
            "memory": 1024,
            "maxDuration": 300
        }
    },
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
}
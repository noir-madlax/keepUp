{
    "version": 2,
    "functions": {
        "api/index.py": {
            "memory": 1024,
            "maxDuration": 300
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "LOG_LEVEL": "DEBUG"
    },
    "crons": [
        {
            "path": "/api/router/get",
            "schedule": "0 0 * * *"
        }
    ]
}
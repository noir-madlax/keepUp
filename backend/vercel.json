{
    "version": 2,
    "functions": {
        "app/main.py": {
            "use": "@vercel/python",
            "memory": 1024,
            "maxDuration": 300
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
}